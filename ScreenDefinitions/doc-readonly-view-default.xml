<?xml version="1.0" encoding="UTF-8"?>
<view name="doc-readonly-view-default" readonly="true" template="Doc-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

	<labels default="en">
		<locale name="en">
			<label name="doc" value="Document (Read Only)"/>
			
		</locale>
	</labels>

	<section-overrides>	
			
	</section-overrides>
	
	<property-overrides>
			
			<textArea name="processInformation.comment" readonly="true"  />
			<dropDown name="subType" readonly="true" />
			<dropDown name="sensitivity" readonly="true" />
			<date name="date" labelKey="Submitted Date" readonly="true" />
			<checkbox name="tbdBoolean1" labelKey="InActive" readonly="true" />
			<date name="tbdDate1" labelKey="Signature Date" readonly="true"/>
			<dropDown name="tbdString1" labelKey="Legal Entity" readonly="true"/>
			<date name="tbdDate2" labelKey="Expiration Date" readonly="true" />
			<textArea name="voucherComments" labelKey="Comments" readonly="true" />
			<text name="vendor.name" validation="" readonly="true"/>		
		
	</property-overrides>

	<lineItem-overrides>
			 
	</lineItem-overrides>

	<action-overrides>
			<getNext name="next" readonly="true" visible="false" />
			<rejectButton name="reject" readonly="true"  />
			<button name="update" readonly="true" />
			<button name="forward" readonly="true" />
		
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="true" note="message.returnException">
				<subReason name="Duplicate Tax Form"/>
				<subReason name="Incomplete documentation"/>
				<subReason name="Not a Genuine Tax Form"/>
				<subReason name="Other"/>
			</reason>
			<reason name="Rescan" commentsRequired="true" note="message.rescan">
			</reason>
			<reason name="Vendor Maintenance" commentsRequired="true" note="message.returnVendorMaintenance">
			</reason>
			<reason name="Terminate" commentsRequired="true" note="message.terminate">
			</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.validVendor" messageType="warning" note="message.validVendorValidation"/>
	</validations>
</view>
