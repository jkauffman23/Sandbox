<?xml version="1.0" encoding="UTF-8"?>
<view name="batch-readonly-view" template="batch-readonly-tpl-001.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd" readonly="true">

	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	<section-overrides>	
	</section-overrides>
	
	<property-overrides>
	</property-overrides>

	<lineItem-overrides> 
	</lineItem-overrides>

	<action-overrides>
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="true" note="message.returnBatch.Exception">
			</reason>
			<reason name="Terminate" commentsRequired="true" note="message.Batch.terminate">
			</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.duplicateInvoiceCheck" messageType="warning" note="message.duplicateInvoiceValidation"/>

	</validations>
</view>
