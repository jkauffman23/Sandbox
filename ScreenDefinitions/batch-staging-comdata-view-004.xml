<?xml version="1.0" encoding="UTF-8"?>
<view name="batch-staging-view-004" template="batch-check-comdata-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd" readonly="false" >

	<labels default="en">
		<locale name="en">
			<label name="release" value="Close"/>
			<label name="batch" value="Funds Payment - Check" />

		</locale>
	</labels>

	<section-overrides>	
		<section name="doc" visible="true"> </section>

	</section-overrides>
	
	<property-overrides>
	
		
	</property-overrides>

	<lineItem-overrides> 
	</lineItem-overrides>

	<action-overrides>
			<rejectButton name="reject" visible="false" />
			<button name="release" visible="true"  />
			<button name="updateRelease" visible="false" />
			<button name="update" visible="false" />
			<button name="forward" visible="false" />
			<button name="getLines" visible="false" />
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
		<hidden name="type" />
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="true" note="message.Batch.terminate">
			</reason>
			<!--reason name="Terminate" commentsRequired="true" note="message.Batch.terminate">
			</reason-->
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.duplicateInvoiceCheck" messageType="warning" note="message.duplicateBatchValidation"/>

	</validations>
</view>
